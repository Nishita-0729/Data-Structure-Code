class Solution {
    public int[] successfulPairs(int[] spells, int[] potions, long success) {
        Arrays.sort(potions);
        int n=spells.length;
        int m=potions.length;
        int[] ans=new int[n];

        for(int i=0;i<n;i++){
            int spell=spells[i];
            int low=0;
            int high=m-1;
            int firstindex=m;

            while(low<=high){
                int mid=(low+high)/2;
                long product=(long) spell * potions[mid];

                if(product>=success){
                    firstindex=mid;
                    high=mid-1;
                }
                else{
                    low=mid+1;
                }
            }
            ans[i]=m-firstindex;
        }
        return ans;
    }
}
